{
  "name": "botify-contest-platform",
  "version": "0.0.0",
  "description": "Botify Contest Platform using React, Express and Docker.",
  "scripts": {
    "clean": "rimraf dist",
    "install:db": "node ./scripts/installDB",
    "install:tmp": "mkdir tmp || true",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "build:docker:js": "docker build -t botify/contest/js -f challenges/js/Dockerfile challenges",
    "build:docker:python": "docker build -t botify/contest/python -f challenges/python/Dockerfile challenges",
    "build:docker": "npm run build:docker:js && npm run build:docker:python",
    "postinstall": "npm run install:tmp && npm run install:db && npm run build:docker",
    "start": "node start.js",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/botify-labs/contest.git"
  },
  "keywords": [
    "botify",
    "react",
    "reactjs",
    "playground",
    "contest",
    "platform",
    "docker"
  ],
  "author": "Botify <labs@botify.com> (http://github.com/botify)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/botify-labs/contest/issues"
  },
  "homepage": "https://github.com/botify-labs/contest",
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "^6.6.5",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^4.2.2",
    "estraverse": "^4.2.0",
    "estraverse-fb": "^1.3.1",
    "eventsource-polyfill": "^0.9.6",
    "file-loader": "^0.8.5",
    "json-loader": "^0.5.4",
    "node-sass": "^3.4.2",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.4.3",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.9.1"
  },
  "dependencies": {
    "ace-builds": "^1.2.2",
    "ansi-to-html": "^0.4.1",
    "body-parser": "^1.15.0",
    "bootstrap": "^3.3.6",
    "classnames": "^2.2.3",
    "email-validator": "^1.0.4",
    "express": "^4.13.3",
    "font-awesome": "^4.5.0",
    "react": "^0.14.7",
    "react-ace-async": "botify-labs/react-ace-async#master",
    "react-dom": "^0.14.7",
    "sqlite3": "^3.1.1",
    "superagent": "^1.8.1"
  }
}
